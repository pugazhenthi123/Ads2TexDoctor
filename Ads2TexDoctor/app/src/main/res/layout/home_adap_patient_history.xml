<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout
    android:id="@+id/home_adap_history_title_lyt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:visibility="gone">
    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight=".25"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/home_adap_history_title_date_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fadingEdge="horizontal"
            android:maxLines="1"
            android:scrollHorizontally="true"
            android:text="Date"
            android:textAlignment="center"
            android:textColor="#303F9F"
            android:textSize="15sp"
            android:textStyle="bold" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight=".25"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/home_adap_history_title_temperature_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fadingEdge="horizontal"
            android:maxLines="1"
            android:scrollHorizontally="true"
            android:text="Temperature"
            android:textAlignment="center"
            android:textColor="#303F9F"
            android:textSize="15sp"
            android:textStyle="bold" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight=".25"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/home_adap_history_title_sugar_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fadingEdge="horizontal"
            android:maxLines="1"
            android:scrollHorizontally="true"
            android:text="Sugar"
            android:textAlignment="center"
            android:textColor="#303F9F"
            android:textSize="15sp"
            android:textStyle="bold" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight=".25"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/home_adap_history_title_pressure_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fadingEdge="horizontal"
            android:maxLines="1"
            android:scrollHorizontally="true"
            android:text="Pressure"
            android:textAlignment="center"
            android:textColor="#303F9F"
            android:textSize="15sp"
            android:textStyle="bold" />
    </LinearLayout>
</LinearLayout>
    <LinearLayout
        android:id="@+id/home_adap_history_lyt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight=".25"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/home_adap_history_date_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fadingEdge="horizontal"
                android:maxLines="1"
                android:scrollHorizontally="true"
                android:text="Date"
                android:textAlignment="center"
                android:textColor="@color/carbon_black"
                android:textSize="13sp"
                android:textStyle="bold" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight=".25"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/home_adap_history_temperature_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fadingEdge="horizontal"
                android:maxLines="1"
                android:scrollHorizontally="true"
                android:text="Temperatue"
                android:textAlignment="center"
                android:textColor="@color/carbon_black"
                android:textSize="13sp"
                android:textStyle="bold" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight=".25"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/home_adap_history_sugar_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fadingEdge="horizontal"
                android:maxLines="1"
                android:scrollHorizontally="true"
                android:text="Sugar"
                android:textAlignment="center"
                android:textColor="@color/carbon_black"
                android:textSize="13sp"
                android:textStyle="bold" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight=".25"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/home_adap_history_pressure_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fadingEdge="horizontal"
                android:maxLines="1"
                android:scrollHorizontally="true"
                android:text="Pressure"
                android:textAlignment="center"
                android:textColor="@color/carbon_black"
                android:textSize="13sp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>